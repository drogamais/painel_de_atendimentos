	column 'Dias desde a última' = ```
			
			VAR LOJA = Tarefa[projeto_id]
			VAR SERVICO = Tarefa[fase_nome]
			VAR TAREFAS= Tarefa[nome]
			
			VAR DATA_TAREFA =
			    CALCULATE(
			        MAX(Tarefa[dtConclusao]),
			        FILTER(
			            ALL(Tarefa),
			            Tarefa[projeto_id] = LOJA &&
			            Tarefa[fase_nome] = SERVICO &&
			            Tarefa[nome] = TAREFAS
			        )
			    )
			
			VAR DATA_TENTATIVA =
			    CALCULATE(
			        MAX(Tarefa[dtConclusao]),
			        FILTER(
			            ALL(Tarefa),
			            Tarefa[projeto_Id] = LOJA &&
			            Tarefa[fase_nome] = SERVICO &&
			            Tarefa[nome] = "TENTATIVA"
			        )
			    )
			
			VAR DIAS_DA_TENTATIVA = DATEDIFF(DATA_TENTATIVA, TODAY(), DAY)
			
			VAR DIAS = DATEDIFF(DATA_TAREFA, TODAY(), DAY)
			
			RETURN
			    SWITCH(
			        TRUE(),
			        Tarefa[Entrou em Caducidade] = "SIM", 0,
			        Tarefa[possui_tentativas] = "SIM" && Tarefa[nome] <> "TENTATIVA", DIAS_DA_TENTATIVA + 15,
			        DIAS
			    )
			
			```
		formatString: 0
		lineageTag: a3be8d98-4af9-4b3b-b4e2-8c7688a92334
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column 'Entrou em Caducidade' = ```
			
			VAR FaseAtual = Tarefa[fase_nome]
			VAR LojaAtual = Tarefa[LOJA]
			
			VAR ExisteCaducidade = 
			    CALCULATE(
			        COUNTROWS(Tarefa),
			        ALLEXCEPT(Tarefa, Tarefa[LOJA], Tarefa[fase_nome]),
			        Tarefa[nome] = "CADUCIDADE"
			    )
			
			RETURN IF(ExisteCaducidade > 0 && Tarefa[fase_nome] = FaseAtual && Tarefa[LOJA] = LojaAtual, "SIM", "NÃO")
			```
		lineageTag: f07988cc-855b-48b4-9aad-6b55f1da6cf1
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

