	column possui_tentativas =
			
			
			VAR LOJA = Tarefa[LOJA]
			VAR FASE = Tarefa[fase_nome]
			
			VAR qtd_tentativas = CALCULATE(
			                        COUNT(Tarefa[id]),
			                        FILTER(
			                            ALL(Tarefa),
			                            Tarefa[LOJA] = LOJA &&
			                            Tarefa[fase_nome] = FASE &&
			                            Tarefa[nome] = "TENTATIVA"
			                        )
			                    )
			RETURN IF(qtd_tentativas>0, "SIM", "NAO")
